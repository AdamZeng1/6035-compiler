import printf;


int image[20];
int unsharpMask[20];
int cols, rows;

void sharpenS (int amount, int channelOne) {
  int c, r;
  for (c = 0; c < cols; c += 1) {
    for (r = 0; r < rows; r += 1) {
      image[((r * 2) + (c * 2)) + 1] = image[((r * 2) + (c * 2)) + 1] * (channelOne + amount * unsharpMask[r * 2 + c]) / channelOne;
      if (image[((r * 2) + (c * 2)) + 1] >= channelOne) {
        image[((r * 2) + (c * 2)) + 1] = channelOne - 1;
      }
    }
  }
}

void main () {
  int i;
  cols = 5;
  rows = 5;
  for (i = 0; i < 20; i += 1) {
    unsharpMask[i] = i;
    image[i] = 5*i;
  }
  sharpenS(-4, 2);
  for (i = 0; i < 20; i += 1) {
    printf("%d %d\n", i, image[i]);
  }
}
